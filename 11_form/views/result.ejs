    <!-- <%console.log(Object.keys(userInfo).length)%>  -->
    <% if(Object.values(userInfo).length==4){%>
    <div>이메일은 <span><%=userInfo.email%></span>이시네용.</div>
    <div>학년은 <span><%=userInfo.grade%></span>학년이고용</div>
    <% }%>

    <!-- <% if (userInfo.email) { %>
      <div>이메일은 <span><%= userInfo.email %></span>이시네요!</div>
      <% } %> <% if (userInfo.grade) { %>
      <div><span><%= userInfo.grade %></span>학년 이군요!</div>
      <% } %> -->
  </body>
</html>
